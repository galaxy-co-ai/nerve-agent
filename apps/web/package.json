  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "dev:stable": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev --hostname 127.0.0.1 --port 3000",
